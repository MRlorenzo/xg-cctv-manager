<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xg.cctv.mybatis.mapper.JsExceptionMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.xg.cctv.mybatis.po.JsException">
                <id column="exception_id" property="exceptionId" />
                <result column="user_name" property="userName" />
                <result column="create_date" property="createDate" />
                <result column="stack" property="stack" />
                <result column="url" property="url" />
                <result column="message" property="message" />
                <result column="info" property="info" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        exception_id, user_name, create_date, stack, url, message, info
    </sql>
</mapper>
